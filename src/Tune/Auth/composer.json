{
    "name": "tune/auth",
    "description": "The Tune Auth package.",
    "license": "MIT",
    "homepage": "https://harmony.com",
    "support": {
        "issues": "https://github.com/harmony/framework/issues",
        "source": "https://github.com/harmony/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@harmony.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "tune/collections": "^10.0",
        "tune/contracts": "^10.0",
        "tune/http": "^10.0",
        "tune/macroable": "^10.0",
        "tune/queue": "^10.0",
        "tune/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tune\\Auth\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "tune/console": "Required to use the auth:clear-resets command (^10.0).",
        "tune/queue": "Required to fire login / logout events (^10.0).",
        "tune/session": "Required to use the session based guard (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
