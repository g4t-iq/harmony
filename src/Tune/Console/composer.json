{
    "name": "tune/console",
    "description": "The Tune Console package.",
    "license": "MIT",
    "homepage": "https://harmony.com",
    "support": {
        "issues": "https://github.com/harmony/framework/issues",
        "source": "https://github.com/harmony/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@harmony.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "tune/collections": "^10.0",
        "tune/contracts": "^10.0",
        "tune/macroable": "^10.0",
        "harmony/prompts": "^0.1",
        "tune/support": "^10.0",
        "tune/view": "^10.0",
        "nunomaduro/termwind": "^1.13",
        "symfony/console": "^6.2",
        "symfony/process": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Tune\\Console\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "ext-pcntl": "Required to use signal trapping.",
        "dragonmantank/cron-expression": "Required to use scheduler (^3.3.2).",
        "guzzlehttp/guzzle": "Required to use the ping methods on schedules (^7.5).",
        "tune/bus": "Required to use the scheduled job dispatcher (^10.0).",
        "tune/container": "Required to use the scheduler (^10.0).",
        "tune/filesystem": "Required to use the generator command (^10.0).",
        "tune/queue": "Required to use closures for scheduled jobs (^10.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
