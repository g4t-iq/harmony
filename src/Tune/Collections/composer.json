{
    "name": "tune/collections",
    "description": "The Tune Collections package.",
    "license": "MIT",
    "homepage": "https://harmony.com",
    "support": {
        "issues": "https://github.com/harmony/framework/issues",
        "source": "https://github.com/harmony/framework"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@harmony.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "tune/conditionable": "^10.0",
        "tune/contracts": "^10.0",
        "tune/macroable": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tune\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
